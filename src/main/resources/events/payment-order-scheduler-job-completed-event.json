{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "completionDate": {
      "type": "string",
      "format": "date-time"
    },
    "status": {
      "type": "string",
      "enum": ["COMPLETED", "COMPLETED_WITH_ERROR"]
    },
    "errorMessage": {
      "type": "string"
    }
  },
  "required": [
    "completionDate",
    "status"
  ]
}